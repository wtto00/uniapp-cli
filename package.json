{
  "private": true,
  "scripts": {
    "dev": "run-p dev:common dev:android dev:core",
    "dev:common": "pnpm -C packages/common dev",
    "dev:android": "pnpm -C packages/android dev",
    "dev:core": "pnpm -C packages/core dev",
    "uniapp": "tsx packages/core/src/index.ts",
    "dev:docs": "pnpm -C packages/docs docs:dev",
    "build:core": "pnpm -C packages/core build",
    "check": "biome check --write .",
    "preinstall": "npx -y only-allow pnpm",
    "prepare": "husky && echo npx lint-staged > .husky/pre-commit"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.10.5",
    "@wtto00/biome-config": "^0.0.1",
    "globstar": "^1.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "license": "MIT",
  "packageManager": "pnpm@9.15.4",
  "lint-staged": {
    "packages/core/package.json": ["pnpm -C packages/core prebuild", "git add packages/core/src/utils/const.ts"],
    "*": ["biome check --no-errors-on-unmatched --write"]
  }
}
