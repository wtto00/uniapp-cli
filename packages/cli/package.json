{
  "name": "@uniapp-cli/core",
  "version": "0.0.1",
  "description": "CLI for uniapp.",
  "type": "module",
  "bin": {
    "uniapp": "./bin/uniapp.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/index",
    "build": "rm -rf dist && tsc -p tsconfig.json --pretty && node scripts/build.js",
    "build:type": "tsc --outDir dist --declaration --emitDeclarationOnly --pretty --types src/global.d.ts src/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wtto00/uniapp-cli.git",
    "directory": "packages/cli"
  },
  "keywords": [
    "uniapp",
    "cli"
  ],
  "author": "wtto00 <wtto00@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wtto00/uniapp-cli/issues"
  },
  "homepage": "https://github.com/wtto00/uniapp-cli#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "degit": "^2.8.4",
    "inquirer": "^9.2.12",
    "miniprogram-automator": "^0.12.1",
    "open": "^10.0.3",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/degit": "^2.8.6",
    "@types/inquirer": "^9.0.7",
    "pkg-types": "^1.0.3"
  }
}
